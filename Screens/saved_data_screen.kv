<SavedDataScreen>:
    name: 'saved_data'
    id: saved_data_screen

    MDBoxLayout:
        orientation: "vertical"
        padding: [dp(5), dp(5), dp(5), dp(5)]
        spacing: dp(10)
        md_bg_color: (0, 0, 1, 1)

        MDTopAppBar:
            type: "small"
            size_hint_x: 1
            # size_hint_y: 1
            # pos_hint: {"center_x": .5, "center_y": .5}
            theme_bg_color: "Custom"
            md_bg_color: (1,1,1,1)
            

            MDTopAppBarLeadingButtonContainer:

                MDActionTopAppBarButton:
                    icon: "arrow-left"
                    # icon_color: 1,1,1,1

                    on_release: 
                        app.root.ids.nav_screen_manager.current = 'blank_screen' 
                        app.root.ids.screen_manager.current = 'Home' 

            MDTopAppBarTitle:
                text: "Storage"
                # text_color: 1,1,1,1

            MDTopAppBarTrailingButtonContainer:

                MDActionTopAppBarButton:
                    icon: "attachment"
                    # icon_color: 1,1,1,1

                MDActionTopAppBarButton:
                    icon: "calendar"
                    # icon_color: 1,1,1,1


                MDActionTopAppBarButton:
                    icon: "dots-vertical"
